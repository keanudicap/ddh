mv results_* bgm/

./runScenarios4ERR experiments/scenarios/movingai/rooms/ "-cardinal -pr"
mkdir rooms512
rm rooms512/*
mv results_* rooms512/

./runScenarios4ERR experiments/scenarios/movingai/mazes/ "-cardinal -pr"
mkdir mazes512
rm mazes512/*
mv results_* mazes512/

tar czf results_bgm_4err_pr.tgz bgm/results*
tar czf results_dao_4err_pr.tgz dao/results*
tar czf results_rooms512_4err_pr.tgz rooms512/results*
tar czf results_mazes512_4err_pr.tgz mazes512/results*
mv *4err_pr.tgz ~/src/ahastar/experiments/results

./runScenarios4ERR experiments/scenarios/movingai/dao/ "-cardinal -bfr"
mkdir dao
rm dao/*
mv results_* dao/

./runScenarios4ERR experiments/scenarios/movingai/bgmaps/ "-cardinal -bfr"
mkdir bgm
rm bgm/*
mv results_* bgm/

./runScenarios4ERR experiments/scenarios/movingai/rooms/ "-cardinal -bfr"
mkdir rooms512
rm rooms512/*
mv results_* rooms512/

./runScenarios4ERR experiments/scenarios/movingai/mazes/ "-cardinal -bfr"
mkdir mazes512
rm mazes512/*
mv results_* mazes512/

tar czf results_bgm_4err_bfr.tgz bgm/results*
tar czf results_dao_4err_bfr.tgz dao/results*
tar czf results_rooms512_4err_bfr.tgz rooms512/results*
tar czf results_mazes512_4err_bfr.tgz mazes512/results*
mv *4err_bfr.tgz ~/src/ahastar/experiments/results

./runScenarios4ERR experiments/scenarios/movingai/dao/ "-cardinal -pr -bfr"
mkdir dao
rm dao/*
mv results_* dao/

#./runScenarios4ERR experiments/scenarios/movingai/bgmaps/ "-cardinal -pr -bfr"
#mkdir bgm
#rm bgm/*
#mv results_* bgm/

./runScenarios4ERR experiments/scenarios/movingai/rooms/ "-cardinal -pr -bfr"
