#mv results_* bgm/
#./runScenarios4ERR experiments/scenarios/movingai/rooms/ "-cardinal "
#mkdir rooms512
#rm rooms512/*
#mv results_* rooms512/
./runScenarios4ERR experiments/scenarios/movingai/mazes/ "-cardinal "
mkdir mazes512
