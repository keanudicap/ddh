#!/bin/bash
# 4C nopr nobfr
for i in $(ls results_*); do
	mv $i bak.$i;
done

for i in $(ls preproc_*); do
	mv $i bak.$i;
done

#./runScenarios4ERR experiments/scenarios/movingai/dao/ "-cardinal "
