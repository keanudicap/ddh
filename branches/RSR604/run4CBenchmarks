#tar czf results_bgm_4err.tgz bgm/results*
#tar czf results_dao_4err.tgz dao/results*
#tar czf results_rooms512_4err.tgz rooms512/results*
tar czf results_mazes512_4err.tgz mazes512/results*
mv *4err.tgz ~/src/ahastar/experiments/results

./runScenarios4ERR experiments/scenarios/movingai/dao/ "-cardinal -pr"
mkdir dao
rm dao/*
mv results_* dao/

./runScenarios4ERR experiments/scenarios/movingai/bgmaps/ "-cardinal -pr"
