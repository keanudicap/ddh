@article{rego01,
	Annote = {Describes the TABUCHAIN algorithm. Uses a node-based ejection chain approach (where an ejection chain is a disjointed multi-level structure of neighbouring vertex triples) which features 2 types of trial moves. The first keeps the same # of routes and the second can grow or reduce the # of routes. 
This is combined with a Tabu Search that makes use of both short term and long term memory (through strategic oscillation) and a post-optimisation phase that tries to improve each individual tour further after a solution to the VRP is found. 
Parallel and sequential implementations are discussed; the former is slightly more efficient.},
	Author = {C. Rego},
