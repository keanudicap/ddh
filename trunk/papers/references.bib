@inproceedings{rintannen03,
	author    = {Jussi Rintanen},
	title     = {Symmetry Reduction for SAT Representations of Transition
		   Systems},
	booktitle = {ICAPS},
	year      = {2003},
	pages     = {32-41},
	annote = {
		Identifies symmetries in transtion systems by looking for interchangeable objects 
		in a given schemata (as a preprocessing step). To show a symmetry exists, the method must check
		that the truth values of variables are invariant wrt. whichever transition-relation is chosen.
		When a symmetry is found, a constraint is posted to break it. This involves defining a < order
		over the interchangeable objects.
		Constraints are translated to the propositional logic where planning-as-SAT is used to solve
		the problem.
		Very similar to shallow pruning in pochter11 but appears weaker.	
	}
			
}

@article{felner07,
  author    = {Ariel Felner and Richard E. Korf and Ram Meshulam and
               Robert C. Holte},
  title     = {Compressed Pattern Databases},
  journal   = {J. Artif. Intell. Res. (JAIR)},
  volume    = {30},
  year      = {2007},
  pages     = {213-247},
  annote = {Discusses lossy and lossless compression techniques. The most interesting include: a lossy compression scheme that maps to the same index two 
	  patterns if they only differ in the value of some least-signficant variable (assuming some lex-order on the variables in the pattern), 
	  a lossless compression scheme that maps cliques of similar patterns to a single index (usually the min-cost pattern in the set) + the number of moves
	  needed to reach any other pattern in the clique (seems like you need to know/compute also the local clique of each pattern with this approach).
	  Finally, delta-PDBs are proposed, which only store deltas between a pattern and some standard heuristic like Manhattan distance.}


}

@inproceedings{zhou03,
  author    = {Rong Zhou and Eric A. Hansen},
  title     = {Sparse-Memory Graph Search},
  booktitle = {IJCAI},
  year      = {2003},
  pages     = {1259-1268},
  bibsource = {DBLP, http://dblp.uni-trier.de}
  annote = {Proposes a mechanism for pruning the closed list. When the memory limit is reached,
	  all nodes from closed are deleted except special "relay" nodes (which have ungenerated predecessors).
	  The parent of every node on open is modified mapped to such an ancentral relay node (possibly the start state)
	  and the search continues. When the goal is found a sparse path, consisting of relay nodes, is returned 
	  and must be refined via further search. }
}

@inproceedings{taylor93
  author    = {Larry A. Taylor and Richard E. Korf},
  title     = {Pruning Duplicate Nodes in Depth-First Search},
  booktitle = {AAAI},
  year      = {1993},
  pages     = {756-761},
  annote = {Excellent paper that describes a mechanism for the automatic construction of duplictae-detecting FSMs. 
	  An "exploratory" BFS is initially performed over the search space up to some max depth. The BFS tree is analysed in order to identify 
	  duplicate nodes s.t. if a node is found via two distinct paths, only one path is kept: the one which respects some given operator ordering.
	  The sequence of operators on the other (rejected) path are used to construct a duplicate-detecting automaton. Any time a node is reached via
	  some path in this automaton it can be pruned because an alternative path exists that respects operator ordering.
  }
}

@inproceedings{culberson96,
  author    = {Joseph C. Culberson and
               Jonathan Schaeffer},
  title     = {Searching with Pattern Databases},
  booktitle = {Canadian Conference on AI},
  year      = {1996},
  pages     = {402-416},
  ee        = {http://dx.doi.org/10.1007/3-540-61291-2_68},
  annote = {The original PDB paper. Defines patterns as partial specifications of the goal state. i.e. a subset of the goal variables.
	  In the case of the 15 puzzle, a partial specification is a subset of the tiles (say, {1, 2, 3, 4}). A PDB then is a database that contains,
	  for all possible arrangements (== assignments) of these variables, an exact cost for moving them all back to the goal position.
	  The PDB is then used as a more accurate heuristic during search. To reduce the size of PDBs, patterns that are mirrors of each other (diagonal,
	  horizontal or vertical reflections) are combined together. 1038x reduction in node expansions but only 12x search time reduction (node expansions are 
	  more expensive vs simpler Manhattan heuristic; lookups are needed etc).}
}

@article{korf02,
  author    = {Richard E. Korf and
