@INPROCEEDINGS{taylor93,
  author = {Larry A. Taylor and Richard E. Korf},
  title = {Pruning Duplicate Nodes in Depth-First Search},
  booktitle = {AAAI},
  year = {1993},
  pages = {756-761},
  annote = {Excellent paper that describes a mechanism for the automatic construction
	of duplictae-detecting FSMs. An "exploratory" BFS is initially performed
	over the search space up to some max depth. The BFS tree is analysed
	in order to identify duplicate nodes s.t. if a node is found via
	two distinct paths, only one path is kept: the one which respects
	some given operator ordering. The sequence of operators on the other
	(rejected) path are used to construct a duplicate-detecting automaton.
	Any time a node is reached via some path in this automaton it can
	be pruned because an alternative path exists that respects operator
	ordering. }
}

