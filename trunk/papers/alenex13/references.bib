@INPROCEEDINGS{culberson96,
  author = {Joseph C. Culberson and Jonathan Schaeffer},
  title = {Searching with Pattern Databases},
  booktitle = {Canadian Conference on AI},
  year = {1996},
  pages = {402-416},
  annote = {The original PDB paper. Defines patterns as partial specifications
	of the goal state. i.e. a subset of the goal variables. In the case
	of the 15 puzzle, a partial specification is a subset of the tiles
	(say, {1, 2, 3, 4}). A PDB then is a database that contains, for
	all possible arrangements (== assignments) of these variables, an
	exact cost for moving them all back to the goal position. The PDB
	is then used as a more accurate heuristic during search. To reduce
	the size of PDBs, patterns that are mirrors of each other (diagonal,
	horizontal or vertical reflections) are combined together. 1038x
	reduction in node expansions but only 12x search time reduction (node
	expansions are more expensive vs simpler Manhattan heuristic; lookups
	are needed etc).},
  ee = {http://dx.doi.org/10.1007/3-540-61291-2_68}
}

