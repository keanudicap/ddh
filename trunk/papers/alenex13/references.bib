@ARTICLE{felner07,
  author = {Ariel Felner and Richard E. Korf and Ram Meshulam and Robert C. Holte},
  title = {Compressed Pattern Databases},
  journal = {J. Artif. Intell. Res. (JAIR)},
  year = {2007},
  volume = {30},
  pages = {213-247},
  annote = {Discusses lossy and lossless compression techniques. The most interesting
	include: a lossy compression scheme that maps to the same index two
	patterns if they only differ in the value of some least-signficant
	variable (assuming some lex-order on the variables in the pattern),
	a lossless compression scheme that maps cliques of similar patterns
	to a single index (usually the min-cost pattern in the set) + the
	number of moves needed to reach any other pattern in the clique (seems
	like you need to know/compute also the local clique of each pattern
	with this approach). Finally, delta-PDBs are proposed, which only
	store deltas between a pattern and some standard heuristic like Manhattan
	distance.}
}

