tablenames <- c("id", "name", "ne", "nt", "ng", "st", "opl", "map")
tableclasses <- c("integer", "character", "integer", "integer", "integer", "double", "double", "character")
hpanames <- c("exp", "name", "ne", "nt", "pm", "st", "insne", "insnt", "inspm", "insst", "opl", "map")
hpaclasses <- c("integer", "character", "integer", "integer", "integer", "double", "integer", "integer", "integer", "double", "double", "character")
# these names are for Swamps results
jastarnames <- c("id", "name", "ne", "nt", "ng", "st", "opl", "map")
jastarclasses <- c("integer", "character", "integer", "integer", "integer", "double", "double", "character")

bgm_astar <- runav(loadResults("data/bgm/results_JPS", tablenames, tableclasses), 3)
bgm_JPA <- runav(loadResults("data/bgm/results_JPS", tablenames, tableclasses), 3)
bgm_JPS <- runav(loadResults("data/bgm/results_JPS", tablenames, tableclasses), 3)
bgm_speedup_jps <- speedup(bgm_jps, bgm_astar)
bgm_speedup_jpa <- speedup(bgm_jpa, bgm_astar)
bgm_speedup_jpa_jps <- speedup(bgm_jpa, bgm_jps)

dao_astar <- runav(loadResults("data/dao/results_JPS", tablenames, tableclasses), 3)
dao_JPA <- runav(loadResults("data/dao/results_JPS", tablenames, tableclasses), 3)
dao_JPS <- runav(loadResults("data/dao/results_JPS", tablenames, tableclasses), 3)
dao_speedup_jps <- speedup(dao_jps, dao_astar)
dao_speedup_jpa <- speedup(dao_jpa, dao_astar)
dao_speedup_jpa_jps <- speedup(dao_jpa, dao_jps)

mazes_astar <- runav(loadResults("data/mazes/results_JPS", tablenames, tableclasses), 3)
mazes_JPA <- runav(loadResults("data/mazes/results_JPS", tablenames, tableclasses), 3)
mazes_JPS <- runav(loadResults("data/mazes/results_JPS", tablenames, tableclasses), 3)
mazes_speedup_jps <- speedup(mazes_jps, mazes_astar)
mazes_speedup_jpa <- speedup(mazes_jpa, mazes_astar)
mazes_speedup_jpa_jps <- speedup(mazes_jpa, mazes_jps)

rooms_astar <- runav(loadResults("data/rooms/results_JPS", tablenames, tableclasses), 3)
rooms_JPA <- runav(loadResults("data/rooms/results_JPS", tablenames, tableclasses), 3)
rooms_JPS <- runav(loadResults("data/rooms/results_JPS", tablenames, tableclasses), 3)
rooms_speedup_jps <- speedup(rooms_jps, rooms_astar)
rooms_speedup_jpa <- speedup(rooms_jpa, rooms_astar)
rooms_speedup_jpa_jps <- speedup(rooms_jpa, rooms_jps)
